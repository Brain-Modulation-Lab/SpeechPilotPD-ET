incE = {}; n=1; gp = groups{gg};
for ff=1:length(sd.(gp).freq)
    for ll=1:length(sd.(gp).freq(ff).loc)
        for ss=1:length(sd.(gp).freq(ff).loc(ll).include)
            inc = find(sd.(gp).freq(ff).loc(ll).include{ss});
            if ~isempty(inc)
                for ii=1:length(inc)
                    incE{n} = sprintf('S%dE%d', ss, inc(ii));
                    n = n+1;
                end
            end
        end
    end
end
